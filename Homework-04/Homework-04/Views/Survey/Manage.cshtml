@section Scripts {
    @Scripts.Render("~/bundles/SurveyMange")
}

<div class="col-sm-12" style="float:left;margin:20px" >
    <h3 style="margin-bottom:10px;margin-top:10px">Surveys Published</h3>
    <table class="table table-hover table-bordered table-striped" id="surveysTable">
        <thead>
            <tr>
                <th>Survey Id</th>
                <th>Study Group Name</th>
                <th>Survey Created Time</th>
            </tr>
        </thead>

    </table>
    <h3 style="margin-bottom:10px;margin-top:10px">Generate Reports</h3>
    <div class="col" style="float:left; margin:10px">
        <button type="submit" id="navigateToSurveyManager1" class="btn btn-default">Get Report for Study group-1</button>
    </div>
    <div class="col" style="float:left; margin:10px">
        <button type="submit" id="navigateToSurveyManager2" class="btn btn-default">Get Report for Study group-2</button>
    </div>
    <div class="col" style="float:left; margin:10px">
        <button type="submit" id="navigateToSurveyManager3" class="btn btn-default">Get Report for study group-3</button>
    </div>
</div>